{
    "name": "aweb/nexus",
    "description": "Aweb Mods for OpenCart",
    "type": "library",
    "keywords": [
		"mods",
        "framework",
        "Aweb",
        "Aweb Design",
        "OpenCart"
    ],
    "homepage": "https://www.awebdesign.ro/",
    "version": "2.1.01",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.3",
        "rakit/validation": "^1.4",
        "symfony/var-dumper": "^5.4"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "Aweb\\Nexus\\": "./"
        },
        "files": [
            "Support/Helpers/common.php",
            "Support/Helpers/nexus.php",
            "Support/Helpers/utils.php"
        ],
        "classmap": [
        ]
    }
}
